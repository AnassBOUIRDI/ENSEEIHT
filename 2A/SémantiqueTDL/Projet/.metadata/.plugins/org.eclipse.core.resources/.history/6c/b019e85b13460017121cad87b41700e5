package fr.n7.stl.block.ast;

import fr.n7.stl.util.SymbolTable;

/**
 * @author Marc Pantel
 *
 */
public interface Instance extends Expression {
	
	/**
	 * Adds an expression at the end of a sequence of expression.
	 * @param _value AST of the expression added to the sequence.
	 */
	public SymbolTable<Declaration> getSymbolTable();

}
